<h1> Creating a child</h1>
<%= form_with model: @children do |form| %>
	<div>
		<%= form.label :First_Name %>
		<%= form.text_field :first_name %>
	</div><br>
	<div>
		<%= form.label :Last_Name %>
		<%= form.text_field :last_name %>
	</div><br>
	<div>
		<%= form.label :E_mail %>
		<%= form.email_field :mail %>
	</div><br>
	<div>

		<%= form.submit :submit %>
	</div>
<% end %>
