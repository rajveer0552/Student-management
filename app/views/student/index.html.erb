 <h1> Child Detail </h1>
 <table>
 	<thead>
 	
 	<tr>
 		<th>
 			First Name :-
 		</th>
 		<th>
 			Last Name :-
 		</th>
 		<th>
 			Mail :-
 		</th>
 	</tr>
 	</thead>
 	<tbody>
 		<% @children.each do |child| %>
 		<tr>
 			<td> <%= child.first_name %></td>
 			<td> <%= child.last_name %></td>
 			<td> <%= child.mail %></td>
  			<% end %>
  		</tr>
 	</tbody>
 </table>
